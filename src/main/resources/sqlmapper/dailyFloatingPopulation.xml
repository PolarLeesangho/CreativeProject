<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.DFPMapper">
    <resultMap id="result" type="DB.DTO.DailyFloatingPopulationDTO">
        <result property="id" column="id"/>
        <result property="year" column="year"/>
        <result property="month" column="month"/>
        <result property="dongCode" column="dong_code"/>
        <result property="dongName" column="dong_name"/>
        <result property="time" column="time"/>
        <result property="dailyFloatingPopulation" column="daily_floating_population"/>
    </resultMap>

    <select id="selectAll" resultMap="result">
        SELECT *
        FROM daily_floating_population
    </select>
</mapper>
