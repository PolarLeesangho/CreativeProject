<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="Boundary.LoginPageController">

    <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <left>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
        </left>
        <right>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
        </right>
        <top>
            <TextField alignment="CENTER" prefHeight="53.0" prefWidth="600.0" text="ManagerLogin" BorderPane.alignment="CENTER" />
        </top>
        <center>
            <GridPane BorderPane.alignment="CENTER">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="id" />
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="pwd" GridPane.rowIndex="1" />
                    <TextField fx:id="id_textField" GridPane.columnIndex="1" />
                    <TextField fx:id="pwd_textField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                </children>
            </GridPane>
        </center>
        <bottom>
            <Pane prefHeight="113.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                <children>
                    <Button fx:id="loginBtn" onAction="#handleloginBtn" layoutX="170.0" layoutY="31.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="77.0" text="로그인" />
                    <Button fx:id="backBtn" onAction="#handleBackBtn" layoutX="380.0" layoutY="31.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="77.0" text="뒤로" />
                </children>
            </Pane>
        </bottom>
    </BorderPane>
</VBox>


